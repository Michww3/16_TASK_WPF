        <Button x:Name="Button_Unistall" Content="Деинсталяция" HorizontalAlignment="Left" Margin="255,349,0,0" VerticalAlignment="Top" Height="49" Width="83" Click="Button_Unistall_Click"/>
        <Button x:Name="Button_Start_Serv" Content="Старт службы" HorizontalAlignment="Left" Margin="456,349,0,0" VerticalAlignment="Top" Height="49" Width="84" Click="Button_Start_Serv_Click"/>
        <Button x:Name="Button_Stop_Serv" Content="Остановка службы" HorizontalAlignment="Left" Margin="646,349,0,0" VerticalAlignment="Top" Height="49" Width="83" Click="Button_Stop_Serv_Click"/>

    </Grid>
</Window>
